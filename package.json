  "private": false,
  "scripts": {
    "bdd-tests": "gt-ui-tests --envVarsPath=.env --headless=true --tests=tests/bdd --reporterOutput=tests/reports/bdd",
    "bdd-tests:dev": "gt-ui-tests --envVarsPath=.env --browser=chrome --liveMode=true --tests=tests/bdd --reporterOutput=tests/reports/bdd",
    "bootstrap-from-sources": "echo 'Not required'",
    "bootstrap-from-sources:dev": "echo 'Not required'",
    "build": "cross-env GENERATE_SOURCEMAP=false react-app-rewired build",
    "build-jutro-modules": "echo 'Not required'",
    "build:dev": "react-app-rewired build --config-overrides ./config-overrides.dev.js",
    "changelog": "jutro-cli generate changelog",
    "depcheck": "jutro-cli check dependencies",
    "e2e-tests": "cross-env CI=true gt-ui-dev-tests --envVarsPath=.env --headless=true --reporterOutput=tests/reports/e2e --tests=tests/e2e",
    "e2e-tests:dev": "gt-ui-dev-tests --envVarsPath=.env --browser=chrome --headless=false --reporterOutput=tests/reports/e2e --tests=tests/e2e",
    "i18n": "rimraf i18n && jutro-cli i18n generate-translation",
    "install-from-sources": "echo 'Not required'",
    "install-from-sources:dev": "echo 'Not required'",
    "install-frontend": "echo 'Not required'",
    "install-jutro-modules": "echo 'Not required'",
    "lerna": "lerna",
    "lerna-init": "node setupLerna",
    "lerna-setup": "echo 'Not required'",
    "lerna-setup:dev": "echo 'Not required'",
    "lint": "cross-env run-s lint:*",
    "lint:css": "cross-env stylelint \"**/*.scss\"",
    "lint:js": "cross-env eslint './**/*.js'",
    "postbuild": "echo 'Not required'",
    "postbuild:dev": "npm run postbuild",
    "postpack": "node postpack.js",
    "prebuild": "npm run write-build-info && npm run i18n",
    "prebuild:dev": "npm run prebuild",
    "prepack": "node prepack.js",
    "prepare-bdd-report": "prepare-bdd-artifact",
    "prepare-visual-report": "prepare-visual-artifact",
    "prestart": "npm run write-build-info && jutro-cli check version && npm run i18n",
    "prestart:dev": "npm run i18n",
    "prettier": "cross-env prettier --write -c '**/*.{ts,tsx,js,json,scss}'",
    "prettier-check": "cross-env prettier --check -c '**/*.{ts,tsx,js,json,scss}'",
    "prettier-check-files": "cross-env prettier --check",
    "sass": "sass --watch src/themes/scss:src/themes/css",
    "serve:dist": "npx http-server -p 6006 dist",
    "start": "react-app-rewired start",
    "start:dev": "react-app-rewired start --config-overrides ./config-overrides.dev.js",
    "test": "react-app-rewired test",
    "test:ci": "cross-env CI=true react-app-rewired test --collectCoverage",
    "test:ci:dev": "npm run test:ci -- --config-overrides ./config-overrides.dev.js",
    "test:dev": "react-app-rewired test --config-overrides ./config-overrides.dev.js",
    "update-app": "npm run update-cli && jutro-cli update app",
    "update-cli": "jutro-cli update cli",
    "update-metadata-schema": "jutro-cli update json-schema",
    "validate-file-structure": "jutro-cli check folder-structure",
    "visual-tests": "cross-env CI=true gt-ui-dev-tests --envVarsPath=.env --browser=chrome --headless=true --reporterOutput=tests/reports/visual --tests=tests/visual/tests",
    "visual-tests:dev": "gt-ui-dev-tests --envVarsPath=.env --browser=chrome --headless=false --reporterOutput=tests/reports/visual --tests=tests/visual/tests",
    "write-build-info": "jutro-cli generate build-info",
    "jutro-cli": "node ./scripts/jutro/jutro-cli.js"
  },
