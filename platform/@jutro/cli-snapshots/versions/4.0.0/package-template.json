{
  "version": "4.0.0",
  "dependencies": {
    "@jutro/app": "4.0.0",
    "@jutro/auth": "4.0.0",
    "@jutro/build-tools": "4.0.0",
    "@jutro/components": "4.0.0",
    "@jutro/config": "4.0.0",
    "@jutro/datatable": "4.0.0",
    "@jutro/layout": "4.0.0",
    "@jutro/locale": "4.0.0",
    "@jutro/logger": "4.0.0",
    "@jutro/overrides": "4.0.0",
    "@jutro/prop-types": "4.0.0",
    "@jutro/floorplan": "4.0.0",
    "@jutro/router": "4.0.0",
    "@jutro/theme": "4.0.0",
    "@jutro/theme-styles": "4.0.0",
    "@jutro/translations": "4.0.0",
    "@jutro/translator": "4.0.0",
    "@jutro/transport": "4.0.0",
    "@jutro/uiconfig": "4.0.0",
    "@jutro/wizard": "4.0.0",
    "@jutro/e2e-tests": "4.0.0",
    "classnames": "2.2.6",
    "core-js": "2.6.9",
    "node-sass": "4.13.1",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-image-gallery": "^1.0.7",
    "react-intl": "2.8.0",
    "react-router-dom": "5.2.0",
    "react-sweet-progress": "^1.1.2"
  },
  "scripts": {
    "sass": "sass --watch src/themes/scss:src/themes/css",
    "prestart": "npm run i18n && npm run write-build-info && jutro-cli check-version",
    "start": "react-app-rewired start",
    "build": "npm run write-build-info && cross-env GENERATE_SOURCEMAP=false react-app-rewired build && npm run i18n",
    "depcheck": "jutro-cli check-dependencies",
    "i18n": "jutro-cli i18n generate-translation",
    "write-build-info": "jutro-cli write-build-info",
    "update-metadata-schema": "jutro-cli update-metadata-schema",
    "update-cli": "jutro-cli update",
    "update-app": "npm run update-cli && jutro-cli update-app",
    "test": "react-app-rewired test",
    "test:ci": "cross-env CI=true react-app-rewired test --collectCoverage",
    "test:ci:dev": "npm run test:ci -- --config-overrides ./config-overrides.dev.js",
    "e2e-tests": "test-runner",
    "visual-tests": "test-runner --visual",
    "prepare-visual-report": "prepare-visual-artifact",
    "lerna": "lerna",
    "validate-file-structure": "node projectStructureValidation",
    "lint:js": "cross-env eslint './**/*.js'",
    "lint:css": "cross-env stylelint \"**/*.scss\"",
    "lint": "cross-env run-s lint:*",
    "prettier": "cross-env prettier --write -c '**/*.{ts,tsx,js}'",
    "prettier-check": "cross-env prettier --check"
  },
  "devDependencies": {
    "@commitlint/cli": "8.1.0",
    "@commitlint/config-conventional": "7.5.0",
    "@commitlint/prompt-cli": "8.2.0",
    "@hot-loader/react-dom": "16.8.6",
    "@jutro/cli": "4.0.0",
    "@jutro/test": "4.0.0",
    "@types/react": "16.9.29",
    "@types/react-router-dom": "^5.1.5",
    "babel-eslint": "10.0.3",
    "babel-plugin-lodash": "3.3.4",
    "babel-plugin-react-intl": "3.0.1",
    "cross-env": "7.0.2",
    "enzyme": "3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "enzyme-to-json": "3.4.0",
    "eslint": "6.8.0",
    "eslint-config-airbnb": "18.0.1",
    "eslint-config-prettier": "6.9.0",
    "eslint-plugin-filenames": "1.3.2",
    "eslint-plugin-import": "2.20.0",
    "eslint-plugin-jsdoc": "22.1.0",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-no-only-tests": "2.4.0",
    "eslint-plugin-react": "7.18.0",
    "eslint-plugin-react-hooks": "2.3.0",
    "fs-extra": "8.1.0",
    "glob": "7.1.3",
    "history": "4.9.0",
    "husky": "1.3.1",
    "identity-obj-proxy": "3.0.0",
    "jest-canvas-mock": "2.1.1",
    "jest-extended": "^0.11.2",
    "jest-teamcity-reporter": "^0.9.0",
    "json5-jest": "1.0.1",
    "json5-loader": "1.0.1",
    "lerna": "^3.21.0",
    "lodash": "4.17.15",
    "npm-run-all": "4.1.5",
    "node-sass-tilde-importer": "1.0.2",
    "postcss-normalize": "8.0.1",
    "prettier": "2.0.1",
    "prop-types": "15.7.2",
    "react-app-rewired": "2.1.5",
    "react-docgen-typescript-loader": "3.7.2",
    "react-scripts": "3.4.0",
    "react-test-renderer": "16.8.6",
    "stylelint": "12.0.1",
    "stylelint-config-standard": "19.0.0",
    "stylelint-declaration-use-variable": "1.7.2",
    "ts-jest": "^25.3.0",
    "ts-loader": "6.2.0",
    "typescript": "3.8.3"
  },
  "engines": {
    "node": "12.14.1"
  }
}
