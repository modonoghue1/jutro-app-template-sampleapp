    "babel-plugin-react-intl": "7.5.0",
    "cross-env": "^7.0.2",
    "cucumber": "6.0.5",
    "enzyme": "3.11.0",
    "enzyme-adapter-react-16": "^1.13.2",
    "enzyme-to-json": "3.6.1",
    "eslint": "7.23.0",
    "eslint-config-airbnb": "18.2.1",
    "eslint-config-prettier": "8.1.0",
    "eslint-plugin-filenames": "1.3.2",
    "eslint-plugin-fp": "2.3.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsdoc": "32.3.0",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-no-only-tests": "2.4.0",
    "eslint-plugin-react": "7.23.1",
    "eslint-plugin-react-hooks": "4.2.0",
    "fs-extra": "9.1.0",
    "gherkin-testcafe": "3.0.1",
    "glob": "7.1.6",
    "husky": "4.3.0",
    "jest-axe": "4.1.0",
    "jest-canvas-mock": "2.3.1",
    "jest-extended": "^0.11.5",
    "jest-watch-typeahead": "0.6.1",
    "jest-teamcity-reporter": "0.9.0",
    "json5-jest": "1.0.1",
    "json5-loader": "4.0.1",
    "lerna": "^3.22.1",
    "lint-staged": "10.5.4",
    "lodash": "4.17.21",
    "node-sass-tilde-importer": "1.0.2",
    "npm-run-all": "4.1.5",
    "postcss-normalize": "9.0.0",
    "prettier": "2.2.1",
    "prop-types": "15.7.2",
    "react-app-rewired": "2.1.8",
    "react-docgen-typescript-loader": "3.7.2",
    "react-scripts": "4.0.3",
    "react-test-renderer": "17.0.2",
    "rimraf": "^3.0.2",
    "stylelint": "13.12.0",
    "stylelint-config-standard": "20.0.0",
    "stylelint-declaration-use-variable": "1.7.2",
    "testcafe": "1.11.0",
    "ts-jest": "26.5.4",
    "ts-loader": "8.1.0",
    "typescript": "4.2.3"
  },
  "packagesToRemove": [
    {
      "packageName": "@jutro/e2e-tests-internal",
      "message": "REMOVED - e2e-tests-internal package has been removed"
    },
    {
      "packageName": "@jutro/lab-preview-title-bars",
      "message": "REMOVED - lab-preview-title-bars package has been removed"
    }
  ]
}
