{
    "jutro": "5.3",
    "claim.confirmation.view": {
        "id": "claimConrimation",
        "type": "page",
        "contentLayout": {
            "component": "Flex",
            "componentProps": {
                "direction": "column",
                "alignItems": "center"
            }
        },
        "content": [
            {
                "id": "confirmationTitle",
                "type": "element",
                "component": "p",
                "content": "If you want to create new FNOL with provided information , please click on Submit",
                "selfLayout": {
                    "component": "FlexItem"
                }
            },
            {
                "id": "loader",
                "type": "element",
                "component": "InlineLoader",
                "componentProps": {
                    "loadingMessage": "Generating FNOL. Please wait..."
                }
            },
            {
                "id": "inlineNotification",
                "type": "element",
                "component": "InlineNotification",
                "componentProps": {
                    "isDismissable": false
                }
            }
        ]
    }
}
